	<queryString>
		<![CDATA[SELECT gestiones_de_escrituras.numero,
historial.fecha,
gestiones_de_escrituras.encabezado,
tipos_de_tramite.nombre AS nombre_tramite,
estados_de_gestion.nombre AS nombre_estado,
gestiones_de_escrituras.numero_archivo,
gestiones_de_escrituras.numero_bibliorato,
gestiones_de_escrituras.observaciones,
personas.nombre AS nombre_cliente_referencia,
personas.apellido AS apellido_cliente_regferencia
FROM historial 
INNER JOIN gestiones_de_escrituras
INNER JOIN estados_de_gestion
INNER JOIN tramites INNER JOIN personas INNER JOIN presupuestos INNER JOIN tipos_de_tramite
WHERE historial.fk_id_gestion = gestiones_de_escrituras.id_gestion
AND historial.fk_id_estado_gestion = estados_de_gestion.id_estado_gestion
AND tramites.fk_id_gestion = historial.fk_id_gestion
AND tramites.fk_id_tipo_tramite = tipos_de_tramite.id_tipo_tramite
AND tramites.fk_id_presupuesto = presupuestos.id_presupuesto
AND presupuestos.fk_id_persona = personas.id_persona
AND historial.fk_id_gestion = $P{idGestion}]]>
	</queryString>
