	<parameter name="numeroGestion" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[0]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT tt.nombre as nombreTramite,dp.nombre as tipoDocumento,
IFNULL (dp.importe_a_pagar ,"No Registra"),
IFNULL (DATE_FORMAT(IFNULL(dp.fecha_pago ,"Pago Pendiente"),'%d/%m/%Y'),"No Registra")

FROM tramites t

INNER JOIN documentos_presentados dp 
ON (t.id_tramite = dp.fk_id_tramite)

INNER JOIN gestiones_de_escrituras ge
ON (t.fk_id_gestion = ge.id_gestion)

INNER JOIN tipos_de_tramite tt
ON (tt.id_tipo_tramite = t.fk_id_tipo_tramite)
WHERE ge.numero = $P{numeroGestion}]]>
	</queryString>
	<field name="nombreTramite" class="java.lang.String"/>
	<field name="tipoDocumento" class="java.lang.String"/>
	<field name="IFNULL (dp.importe_a_pagar ,&quot;No Registra&quot;)" class="java.lang.String"/>
	<field name="IFNULL (DATE_FORMAT(IFNULL(dp.fecha_pago ,&quot;Pago Pendiente&quot;),&apos;%d/%m/%Y&apos;),&quot;No Registra&quot;)" class="java.lang.String"/>
