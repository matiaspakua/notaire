	<queryString>
		<![CDATA[SELECT id_documento_presentado,
gestiones_de_escrituras.id_gestion AS numero_gestion,
gestiones_de_escrituras.encabezado,
personas.nombre AS nombre_cliente_referencia,
personas.apellido AS apellido_cliente_referencia,
documentos_presentados.nombre AS nombre_documento,
documentos_presentados.preparado,
documentos_presentados.fecha_ingreso,
documentos_presentados.fecha_salida,
documentos_presentados.numero_carton,
documentos_presentados.observado,
documentos_presentados.importe_a_pagar,
documentos_presentados.fecha_pago,
documentos_presentados.fecha_liberado,
documentos_presentados.fecha_vencimiento,
documentos_presentados.observaciones
FROM documentos_presentados 
INNER JOIN tramites
INNER JOIN gestiones_de_escrituras
INNER JOIN presupuestos
INNER JOIN personas
WHERE documentos_presentados.id_documento_presentado = $P{idDocumentoPresentado}
AND documentos_presentados.fk_id_tramite = tramites.id_tramite
AND tramites.fk_id_gestion = gestiones_de_escrituras.id_gestion
AND tramites.fk_id_presupuesto = presupuestos.id_presupuesto
AND presupuestos.fk_id_persona = personas.id_persona]]>
	</queryString>
